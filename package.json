{
    "name": "got-server",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "mocha",
        "build": "tsc --sourceMap --outDir ./build/",
        "build:watch": "tsc --sourceMap --watch --outDir ./build/",
        "test:ci": "nyc npm run test",
        "lint": "tslint -c tslint.json -p tsconfig-lint.json src/**/*.ts",
        "lint:fix": "tslint --fix -c tslint.json -p tsconfig-lint.json src/**/*.ts",
        "start": "npm run build:live",
        "build:live": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts --port 80",
        "build-crawler:live": "nodemon --watch src/**/*.ts --exec ts-node src/crawler.ts",
        "build:doc": "node ./node_modules/typedoc/bin/typedoc --out ./tt2/ --mode modules --exclude /**/index ./src/",
        "coverage:remap": "remap-istanbul -i ./coverage/coverage.json -o ./coverage/lcov.info --exclude node_modules --type lcovonly"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/chai": "~4.2.11",
        "@types/cookie-parser": "~1.4.2",
        "@types/expect": "~24.3.0",
        "@types/express": "~4.17.4",
        "@types/jsonwebtoken": "~8.5.0",
        "@types/mocha": "~7.0.2",
        "@types/nconf": "~0.10.0",
        "@types/node": "~14.0.6",
        "@types/passport": "~1.0.3",
        "@types/passport-http-bearer": "~1.0.35",
        "@types/redis": "~2.8.21",
        "@types/serve-static": "~1.13.4",
        "@types/sinon": "~9.0.4",
        "@types/supertest": "~2.0.9",
        "@types/through2": "~2.0.36",
        "argparse": "~1.0.9",
        "body-parser": "~1.19.0",
        "cookie": "~0.4.1",
        "cookie-parser": "~1.4.5",
        "express": "~4.17.1",
        "helmet": "~3.22.0",
        "jsonwebtoken": "~8.5.1",
        "moment": "~2.26.0",
        "nconf": "~0.10.0",
        "passport": "~0.4.0",
        "passport-http-bearer": "~1.0.1",
        "pug": "~3.0.0",
        "redis": "~3.0.2",
        "serve-static": "~1.14.1",
        "through2": "~3.0.0"
    },
    "devDependencies": {
        "chai": "~4.2.0",
        "chai-http": "~4.3.0",
        "grunt": "~1.1.0",
        "grunt-apidoc": "~0.11.0",
        "grunt-contrib-pug": "~2.0.0",
        "grunt-contrib-uglify": "~4.0.0",
        "grunt-jsbeautifier": "~0.2.13",
        "istanbul": "~0.4.5",
        "karma": "~5.0.9",
        "karma-chai": "~0.1.0",
        "karma-mocha": "~2.0.1",
        "karma-sinon": "~1.0.5",
        "mocha": "~7.2.0",
        "nodemon": "~2.0.4",
        "nyc": "~15.1.0",
        "remap-istanbul": "~0.13.0",
        "sinon": "~9.0.2",
        "sinon-chai": "~3.5.0",
        "supertest": "~4.0.2",
        "ts-node": "~8.10.2",
        "tslint": "^6.1.2",
        "typedoc": "~0.17.7",
        "typescript": "~3.9.3"
    },
    "nyc": {
        "include": [
            "src/*.ts",
            "src/**/*.ts"
        ],
        "exclude": [
            "typings"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "json",
            "html",
            "lcov"
        ],
        "all": true
    }
}