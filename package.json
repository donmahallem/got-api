{
    "name": "got-server",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "mocha --config ./.mocharc.yml",
        "build": "tsc --project ./tsconfig.json",
        "build:watch": "tsc --project ./tsconfig.json",
        "test:ci": "nyc npm run test",
        "lint": "tslint -c tslint.json -p tsconfig-lint.json src/**/*.ts",
        "lint:fix": "tslint --fix -c tslint.json -p tsconfig-lint.json src/**/*.ts",
        "start": "npm run build:live",
        "build:live": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts --port 80",
        "build-crawler:live": "nodemon --watch src/**/*.ts --exec ts-node src/crawler.ts",
        "build:doc": "node ./node_modules/typedoc/bin/typedoc --out ./tt2/ --mode modules --exclude /**/index ./src/",
        "coverage:remap": "remap-istanbul -i ./coverage/coverage.json -o ./coverage/lcov.info --exclude node_modules --type lcovonly"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "argparse": "~2.0.1",
        "body-parser": "~1.19.0",
        "cookie": "~0.4.1",
        "cookie-parser": "~1.4.5",
        "express": "~4.17.1",
        "helmet": "~4.6.0",
        "jsonwebtoken": "~8.5.1",
        "moment": "~2.29.1",
        "nconf": "~0.11.2",
        "passport": "~0.4.1",
        "passport-http-bearer": "~1.0.1",
        "pug": "~3.0.2",
        "redis": "~3.1.2",
        "serve-static": "~1.14.1",
        "through2": "~4.0.2"
    },
    "devDependencies": {
        "@types/chai": "4.2.18",
        "@types/cookie-parser": "1.4.2",
        "@types/expect": "24.3.0",
        "@types/express": "4.17.12",
        "@types/jsonwebtoken": "8.5.2",
        "@types/mocha": "8.2.2",
        "@types/nconf": "0.10.0",
        "@types/node": "14.17.3",
        "@types/passport": "1.0.6",
        "@types/passport-http-bearer": "1.0.36",
        "@types/redis": "2.8.29",
        "@types/serve-static": "1.13.9",
        "@types/sinon": "10.0.2",
        "@types/supertest": "2.0.11",
        "@types/through2": "2.0.36",
        "chai": "4.3.4",
        "chai-http": "4.3.0",
        "grunt": "1.4.1",
        "grunt-apidoc": "0.11.0",
        "grunt-contrib-pug": "3.0.0",
        "grunt-contrib-uglify": "5.0.1",
        "grunt-jsbeautifier": "0.2.13",
        "istanbul": "0.4.5",
        "karma": "6.3.4",
        "karma-chai": "0.1.0",
        "karma-mocha": "2.0.1",
        "karma-sinon": "1.0.5",
        "mocha": "9.0.0",
        "nodemon": "2.0.7",
        "nyc": "15.1.0",
        "remap-istanbul": "0.13.0",
        "sinon": "11.1.1",
        "sinon-chai": "3.7.0",
        "supertest": "6.1.3",
        "ts-node": "10.0.0",
        "tslint": "6.1.3",
        "typedoc": "0.20.37",
        "typescript": "3.9.10"
    },
    "nyc": {
        "include": [
            "src/*.ts",
            "src/**/*.ts"
        ],
        "exclude": [
            "typings"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "json",
            "html",
            "lcov"
        ],
        "all": true
    }
}
